  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@lit-labs/scoped-registry-mixin": "^1.0.1",
    "axios": "^1.4.0",
    "custom-card-helpers": "^1.9.0",
    "eta": "./eta-3.0.3.tgz",
    "home-assistant-js-websocket": "^8.0.1",
    "jest": "^29.5.0",
    "lit": "^2.0.0",
    "string.prototype.replaceall": "^1.0.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "yaml": "1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@rollup/plugin-json": "^6.0.0",
