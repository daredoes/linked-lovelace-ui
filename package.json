  "scripts": {
    "test": "jest",
    "make-version": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "prestart": "npm run make-version",
    "prebuild": "npm run make-version",
    "clean": "rimraf dist .parcel-cache",
    "watch": "parcel watch",
    "obuild": "npm run clean && parcel build ",
    "ostart": "parcel",
    "start": "rollup -c --watch",
    "build": "rollup -c"
